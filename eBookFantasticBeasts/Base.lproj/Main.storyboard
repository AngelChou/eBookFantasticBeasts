        <scene sceneID="NSs-wN-55L">
            <objects>
                <viewController hidesBottomBarWhenPushed="YES" id="YY9-eJ-59y" userLabel="人物介紹" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" id="oFs-bP-twb">
                        <rect key="frame" x="0.0" y="0.0" width="414" height="896"/>
                        <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
                        <subviews>
                            <imageView clipsSubviews="YES" userInteractionEnabled="NO" contentMode="scaleAspectFit" horizontalHuggingPriority="251" verticalHuggingPriority="251" fixedFrame="YES" image="Thunderbird" translatesAutoresizingMaskIntoConstraints="NO" id="YAm-cm-Hag">
